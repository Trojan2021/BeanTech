            if Room 2 Window Open:
                Close
            if Middle Fan Open:
                Close
            Turn on Heaters
        else if: Room 1 Warm && Outside Cold:
            if Room 2 Window Open:
                Close
            if Middle Fan Close:
                Open
            Turn on Fan
        else if: Outside Warm:
            if Room 2 Window Closed:
                Open
